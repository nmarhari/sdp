{
  "expo": {
    "name": "CarbCounter",
    "slug": "carbcounter",
    "version": "1.0.0",
    "scheme": "carbcounter",  
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kent.carbCounter",
      "infoPlist": {
        "CFBundleURLSchemes": [
          "carbcounter" 
        ]
      }
    },
    "android": {
      "package": "com.kent.carbCounter",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "carbcounter", 
              "host": "redirect"        
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store" 
    ],
    "experiments": {
      "typedRoutes": true 
    },
    "extra": {
      "eas": {
        "projectId": "f8b6744e-3386-4888-b60a-a81b9559d29a"
      }
    },
    "owner": "cpavlic" 
  }
}
